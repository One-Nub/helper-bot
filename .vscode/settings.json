{
    "cSpell.words": [
        "agelimit",
        "aiohttp",
        "asgi",
        "autodelete",
        "Autoresponder",
        "autoresponse",
        "autoresponses",
        "blacksheep",
        "BLURPLE",
        "gapi",
        "getpid",
        "gifv",
        "googletrans",
        "groupapi",
        "hikari",
        "logchannel",
        "proxied",
        "punc",
        "requote",
        "roblox",
        "Roblox",
        "roleset",
        "rolesets",
        "Sorensen",
        "teamcenter",
        "textdistance",
        "uvicorn",
        "webserver"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/.venv": true,
        "poetry.lock": true,
    },
    "python.analysis.typeCheckingMode": "standard",
    "pylint.args": [
        "--max-line-length=110",
        "--disable=C0413,C0103,C0114,C0301,W0603,W1203"
    ],
    "black-formatter.importStrategy": "fromEnvironment",
    "black-formatter.args": [
        "-l",
        "110"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
    },
    "pylint.cwd": "${workspaceFolder}/src"
}
